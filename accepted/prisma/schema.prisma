// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user_table {
  id Int @id @default(autoincrement())
  fname String 
  lname String
  email String
  joinDate DateTime @default(now())
}

model tasks {
  id Int @id @default(autoincrement())
  taskList String[]
}